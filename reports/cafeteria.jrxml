	<queryString>
		<![CDATA[SELECT 
pv.id,
p.nombre,
p.precio,
pv.cantidad,
IF(pv.tipo_pago =2, 'Tarjeta', 'Efectivo') tipo_pago,
pv.valor_total
FROM  producto_ventas pv
inner join productos p
ON pv.productos_id = p.id
WHERE pv.id = $P{ID_CLIENTE_PAQUETE}
ORDER by  pv.id DESC
LIMIT 1]]>
